{
  "name": "@cotto/sq",
  "description": "sequential queue processor",
  "version": "0.1.0",
  "author": "cotto",
  "files": [
    "es",
    "dist",
    "README.md",
    "LICENCE"
  ],
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cotttpan/sq.git"
  },
  "bugs": {
    "url": "https://github.com/cotttpan/sq/issues"
  },
  "homepage": "https://github.com/cotttpan/sq",
  "dependencies": {
    "@cotto/utils.ts": "^0.0.8",
    "@types/node": "^8.0.26"
  },
  "devDependencies": {
    "@cotto/tslint-config": "^0.0.6",
    "@types/jest": "^20.0.8",
    "@types/sinon": "^2.3.3",
    "jest": "^20.0.4",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "ts-jest": "^20.0.11",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  },
  "keywords": [
    "queue",
    "typescript"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:dist": "tsc",
    "build:es": "tsc --module es2015 --outDir ./es",
    "clean": "run-p clean:**",
    "clean:dist": "rimraf ./dist",
    "clean:es": "rimraf ./es",
    "lint": "tslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "start": "tsc -w",
    "myprepublish": "run-s typecheck clean build",
    "mypublish": "npm run myprepublish && npm publish --access=public",
    "test": "NODE_ENV=test jest --watch"
  }
}